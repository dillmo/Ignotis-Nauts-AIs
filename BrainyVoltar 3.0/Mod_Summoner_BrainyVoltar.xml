<?xml version="1.0" ?>
<enemy>
    <behaviour>
        <root x="110" y="40">
            <normal>
                <condition id="getBoolEquals">
                    <string id="id">init</string>
                    <string id="value" values="yesno">no</string>
                    <normal>
                        <action id="setBool">
                            <string id="id">Summoner</string>
                            <string id="value" values="flagtoggle">yes</string>
                        </action>
                    </normal>
                </condition>
                <action id="executeBehaviourTree">
                    <string id="fileName">Summoner</string>
                    <string id="Minimized">yes</string>
                </action>
                <action id="executeBehaviourTree">
                    <string id="fileName">Mod_VeankoAI_BrainyVoltar</string>
                    <string id="Minimized">yes</string>
                </action>
                <action id="executeBehaviourTree">
                    <string id="fileName">Mod_BotSkill</string>
                    <string id="Minimized">yes</string>
                </action>
                <andblock>
                    <string id="Comment">Heal Wave logic</string>
                    <normal>
                        <action id="pressButton">
                            <string id="button" values="buttons">FACE_LEFT</string>
                            <float id="duration in seconds">0.00</float>
                        </action>
                    </normal>
                    <or>
                        <condition id="isSkillOnButtonAvailable">
                            <string id="button" values="buttons">FACE_LEFT</string>
                        </condition>
                        <condition id="isFromTeam">
                            <string id="team" values="teamswithnumbers">OWN_TEAM</string>
                            <string id="check on" values="targetself">target</string>
                        </condition>
                        <condition id="isTargetInArea">
                            <string id="condition" values="yesno">yes</string>
                            <float id="xOffset">0.00</float>
                            <float id="yOffset">0.00</float>
                            <float id="width">0.64</float>
                            <float id="height">0.64</float>
                            <string id="check line of sight" values="yesno">yes</string>
                            <string id="ignore invisibility" values="yesno">no</string>
                            <string id="Comment">in range</string>
                        </condition>
                    </or>
                </andblock>
                <andblock>
                    <string id="Comment">Suicide Drones logic</string>
                    <normal>
                        <action id="pressButton">
                            <string id="button" values="buttons">FACE_RIGHT</string>
                            <float id="duration in seconds">0.00</float>
                        </action>
                    </normal>
                    <or>
                        <condition id="isSkillOnButtonAvailable">
                            <string id="button" values="buttons">FACE_RIGHT</string>
                        </condition>
                        <condition id="isFromTeam">
                            <string id="team" values="teamswithnumbers">ENEMY_TEAM</string>
                            <string id="check on" values="targetself">target</string>
                        </condition>
                        <condition id="isTargetInArea">
                            <string id="condition" values="yesno">yes</string>
                            <float id="xOffset">0.00</float>
                            <float id="yOffset">0.00</float>
                            <float id="width">0.50</float>
                            <float id="height">0.50</float>
                            <string id="check line of sight" values="yesno">yes</string>
                            <string id="ignore invisibility" values="yesno">no</string>
                            <string id="Comment">in range</string>
                        </condition>
                    </or>
                </andblock>
                <condition id="isSkillOnButtonAvailable">
                    <string id="button" values="buttons">FACE_TOP</string>
                    <string id="Comment">Healbot logic</string>
                    <normal>
                        <condition id="isCharacterInArea">
                            <string id="groups" values="target receive groups" multiselect="true">PLAYERS;;</string>
                            <string id="teams" values="teams" multiselect="true">OWN_TEAM;;</string>
                            <string id="class"></string>
                            <string id="condition" values="charactervaluesCheckable">health minus dot</string>
                            <string id="comparison" values="valuecompare">less or equal</string>
                            <float id="value">75.00</float>
                            <string id="character minimum">1</string>
                            <float id="xOffset">0.00</float>
                            <float id="yOffset">0.00</float>
                            <float id="width">0.60</float>
                            <float id="height">0.60</float>
                            <string id="check line of sight" values="yesno">no</string>
                            <string id="Comment">Unhealthy teammates nearby?</string>
                            <normal>
                                <action id="pressButton">
                                    <string id="button" values="buttons">FACE_TOP</string>
                                    <float id="duration in seconds">0.00</float>
                                </action>
                            </normal>
                        </condition>
                        <andblock>
                            <string id="Comment">Extend pushes</string>
                            <normal>
                                <action id="pressButton">
                                    <string id="button" values="buttons">FACE_TOP</string>
                                    <float id="duration in seconds">0.00</float>
                                </action>
                            </normal>
                            <or>
                                <condition id="isTurretInArea">
                                    <string id="teams" values="teams" multiselect="true">ENEMY_TEAM;;</string>
                                    <string id="health comparison" values="valuecompare">greater or equal</string>
                                    <float id="health">0.00</float>
                                    <float id="xOffset">0.00</float>
                                    <float id="yOffset">0.00</float>
                                    <float id="width">0.50</float>
                                    <float id="height">0.50</float>
                                    <string id="check line of sight" values="yesno">no</string>
                                </condition>
                                <condition id="isInArea">
                                    <string id="condition" values="yesno">yes</string>
                                    <string id="collision groups" values="collison groups" multiselect="true">CREEPS;;</string>
                                    <string id="check green obstacles only" values="yesno">no</string>
                                    <string id="teams" values="teams" multiselect="true">OWN_TEAM;;</string>
                                    <string id="areaName"></string>
                                    <float id="xOffset">0.00</float>
                                    <float id="yOffset">0.00</float>
                                    <float id="width">0.50</float>
                                    <float id="height">0.50</float>
                                    <string id="check line of sight" values="yesno">no</string>
                                    <string id="ignore invisibility" values="yesno">no</string>
                                </condition>
                                <condition id="checkCounter">
                                    <string id="id">state</string>
                                    <string id="value">8</string>
                                    <string id="compare method" values="valuecompare">equal</string>
                                    <string id="Comment">pushTower</string>
                                </condition>
                            </or>
                        </andblock>
                        <condition id="checkCharacterValue">
                            <string id="property" values="charactervaluesCheckable">health minus dot percentage</string>
                            <float id="value">75.00</float>
                            <string id="compare method" values="valuecompare">less or equal</string>
                            <string id="check on" values="targetself">self</string>
                            <string id="Comment">Heal myself</string>
                            <normal>
                                <action id="pressButton">
                                    <string id="button" values="buttons">FACE_TOP</string>
                                    <float id="duration in seconds">0.00</float>
                                </action>
                            </normal>
                        </condition>
                    </normal>
                </condition>
            </normal>
        </root>
    </behaviour>
</enemy>
