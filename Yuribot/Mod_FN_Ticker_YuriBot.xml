<?xml version="1.0" ?>
<enemy>
    <behaviour>
        <root x="110" y="40">
            <normal>
                <condition id="once">
                    <normal>
                        <action id="adjustCounter">
                            <string id="id">ticker</string>
                            <string id="value">0</string>
                            <string id="adjust method" values="valueadjust">set</string>
                        </action>
                    </normal>
                </condition>
                <action id="setBool">
                    <string id="id">2tick</string>
                    <string id="value" values="flagtoggle">no</string>
                </action>
                <action id="setBool">
                    <string id="id">3tick</string>
                    <string id="value" values="flagtoggle">no</string>
                </action>
                <action id="setBool">
                    <string id="id">5tick</string>
                    <string id="value" values="flagtoggle">no</string>
                </action>
                <action id="setBool">
                    <string id="id">10tick</string>
                    <string id="value" values="flagtoggle">no</string>
                </action>
                <action id="adjustCounter">
                    <string id="id">ticker</string>
                    <string id="value">1</string>
                    <string id="adjust method" values="valueadjust">add</string>
                    <string id="Comment">tick-tock...</string>
                </action>
                <orblock>
                    <string id="Comment">Is it the 2nd tick?</string>
                    <string id="Minimized">yes</string>
                    <normal>
                        <action id="setBool">
                            <string id="id">2tick</string>
                            <string id="value" values="flagtoggle">yes</string>
                        </action>
                    </normal>
                    <or>
                        <condition id="checkCounter">
                            <string id="id">ticker</string>
                            <string id="value">2</string>
                            <string id="compare method" values="valuecompare">equal</string>
                        </condition>
                        <condition id="checkCounter">
                            <string id="id">ticker</string>
                            <string id="value">4</string>
                            <string id="compare method" values="valuecompare">equal</string>
                        </condition>
                        <condition id="checkCounter">
                            <string id="id">ticker</string>
                            <string id="value">6</string>
                            <string id="compare method" values="valuecompare">equal</string>
                        </condition>
                        <condition id="checkCounter">
                            <string id="id">ticker</string>
                            <string id="value">8</string>
                            <string id="compare method" values="valuecompare">equal</string>
                        </condition>
                        <condition id="checkCounter">
                            <string id="id">ticker</string>
                            <string id="value">10</string>
                            <string id="compare method" values="valuecompare">equal</string>
                        </condition>
                    </or>
                </orblock>
                <orblock>
                    <string id="Comment">Is it the 3rd tick? Will skip a tick at end of cycle</string>
                    <string id="Minimized">yes</string>
                    <normal>
                        <action id="setBool">
                            <string id="id">3tick</string>
                            <string id="value" values="flagtoggle">yes</string>
                        </action>
                    </normal>
                    <or>
                        <condition id="checkCounter">
                            <string id="id">ticker</string>
                            <string id="value">3</string>
                            <string id="compare method" values="valuecompare">equal</string>
                        </condition>
                        <condition id="checkCounter">
                            <string id="id">ticker</string>
                            <string id="value">6</string>
                            <string id="compare method" values="valuecompare">equal</string>
                        </condition>
                        <condition id="checkCounter">
                            <string id="id">ticker</string>
                            <string id="value">9</string>
                            <string id="compare method" values="valuecompare">equal</string>
                        </condition>
                    </or>
                </orblock>
                <orblock>
                    <string id="Comment">Is it the 5th tick?</string>
                    <string id="Minimized">yes</string>
                    <normal>
                        <action id="setBool">
                            <string id="id">5tick</string>
                            <string id="value" values="flagtoggle">yes</string>
                        </action>
                    </normal>
                    <or>
                        <condition id="checkCounter">
                            <string id="id">ticker</string>
                            <string id="value">5</string>
                            <string id="compare method" values="valuecompare">equal</string>
                        </condition>
                        <condition id="checkCounter">
                            <string id="id">ticker</string>
                            <string id="value">10</string>
                            <string id="compare method" values="valuecompare">equal</string>
                        </condition>
                    </or>
                </orblock>
                <condition id="checkCounter">
                    <string id="id">ticker</string>
                    <string id="value">10</string>
                    <string id="compare method" values="valuecompare">greater or equal</string>
                    <string id="Comment">resets ticker at end of cycle, also trips the 10th tick indicator</string>
                    <normal>
                        <action id="setBool">
                            <string id="id">10tick</string>
                            <string id="value" values="flagtoggle">yes</string>
                        </action>
                        <action id="adjustCounter">
                            <string id="id">ticker</string>
                            <string id="value">0</string>
                            <string id="adjust method" values="valueadjust">set</string>
                        </action>
                    </normal>
                </condition>
            </normal>
        </root>
    </behaviour>
</enemy>
