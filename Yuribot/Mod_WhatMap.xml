<?xml version="1.0" ?>
<enemy>
    <behaviour>
        <root x="110" y="40">
            <normal>
                <condition id="once">
                    <normal>
                        <action id="adjustCounter">
                            <string id="id">CurrentMap</string>
                            <string id="value">0</string>
                            <string id="adjust method" values="valueadjust">set</string>
                            <string id="Comment">Index:    0:Unknown    1:RibbitIV 2:AIStation 3:Sorona 4:Aiguillon</string>
                        </action>
                    </normal>
                </condition>
                <condition id="isInNamedArea">
                    <string id="area name">HEALAREA</string>
                    <string id="team" values="ownenemy">OWN_TEAM</string>
                    <string id="who" values="targetself">self</string>
                    <normal>
                        <condition id="once">
                            <normal>
                                <orblock>
                                    <string id="Comment">Returns true for Ribbit IV and AI Station 404</string>
                                    <normal>
                                        <orblock>
                                            <string id="Comment">Only returns true on AI404</string>
                                            <normal>
                                                <action id="adjustCounter">
                                                    <string id="id">CurrentMap</string>
                                                    <string id="value">2</string>
                                                    <string id="adjust method" values="valueadjust">set</string>
                                                    <string id="Comment">It&apos;s AI Station, woo...</string>
                                                </action>
                                            </normal>
                                            <else>
                                                <action id="adjustCounter">
                                                    <string id="id">CurrentMap</string>
                                                    <string id="value">1</string>
                                                    <string id="adjust method" values="valueadjust">set</string>
                                                    <string id="Comment">It&apos;s Ribbit IV</string>
                                                </action>
                                            </else>
                                            <or>
                                                <condition id="isInArea">
                                                    <string id="condition" values="yesno">yes</string>
                                                    <string id="collision groups" values="collison groups" multiselect="true">INVINCIBLE_OBSTACLES;;DESTROYABLE_OBSTACLES;;</string>
                                                    <string id="check green obstacles only" values="yesno">yes</string>
                                                    <string id="teams" values="teams" multiselect="true">OWN_TEAM;;ENEMY_TEAM;;NEUTRAL_TEAM;;</string>
                                                    <string id="areaName"></string>
                                                    <float id="xOffset">6.05</float>
                                                    <float id="yOffset">2.00</float>
                                                    <float id="width">0.50</float>
                                                    <float id="height">0.30</float>
                                                    <string id="check line of sight" values="yesno">no</string>
                                                    <string id="ignore invisibility" values="yesno">no</string>
                                                </condition>
                                                <condition id="isInArea">
                                                    <string id="condition" values="yesno">yes</string>
                                                    <string id="collision groups" values="collison groups" multiselect="true">INVINCIBLE_OBSTACLES;;DESTROYABLE_OBSTACLES;;</string>
                                                    <string id="check green obstacles only" values="yesno">yes</string>
                                                    <string id="teams" values="teams" multiselect="true">OWN_TEAM;;ENEMY_TEAM;;NEUTRAL_TEAM;;</string>
                                                    <string id="areaName"></string>
                                                    <float id="xOffset">-6.05</float>
                                                    <float id="yOffset">2.00</float>
                                                    <float id="width">0.50</float>
                                                    <float id="height">0.30</float>
                                                    <string id="check line of sight" values="yesno">no</string>
                                                    <string id="ignore invisibility" values="yesno">no</string>
                                                </condition>
                                            </or>
                                        </orblock>
                                    </normal>
                                    <else>
                                        <orblock>
                                            <string id="Comment">Only returns true on Sorona</string>
                                            <normal>
                                                <action id="adjustCounter">
                                                    <string id="id">CurrentMap</string>
                                                    <string id="value">3</string>
                                                    <string id="adjust method" values="valueadjust">set</string>
                                                    <string id="Comment">It&apos;s Sorona</string>
                                                </action>
                                            </normal>
                                            <else>
                                                <action id="adjustCounter">
                                                    <string id="id">CurrentMap</string>
                                                    <string id="value">4</string>
                                                    <string id="adjust method" values="valueadjust">set</string>
                                                    <string id="Comment">It&apos;s Aiguillon</string>
                                                </action>
                                            </else>
                                            <or>
                                                <condition id="isInArea">
                                                    <string id="condition" values="yesno">yes</string>
                                                    <string id="collision groups" values="collison groups" multiselect="true">JUMPPADS;;</string>
                                                    <string id="check green obstacles only" values="yesno">no</string>
                                                    <string id="teams" values="teams" multiselect="true">OWN_TEAM;;ENEMY_TEAM;;NEUTRAL_TEAM;;</string>
                                                    <string id="areaName"></string>
                                                    <float id="xOffset">5.55</float>
                                                    <float id="yOffset">0.95</float>
                                                    <float id="width">1.00</float>
                                                    <float id="height">0.35</float>
                                                    <string id="check line of sight" values="yesno">no</string>
                                                    <string id="ignore invisibility" values="yesno">no</string>
                                                </condition>
                                                <condition id="isInArea">
                                                    <string id="condition" values="yesno">yes</string>
                                                    <string id="collision groups" values="collison groups" multiselect="true">JUMPPADS;;</string>
                                                    <string id="check green obstacles only" values="yesno">no</string>
                                                    <string id="teams" values="teams" multiselect="true">OWN_TEAM;;ENEMY_TEAM;;NEUTRAL_TEAM;;</string>
                                                    <string id="areaName"></string>
                                                    <float id="xOffset">5.55</float>
                                                    <float id="yOffset">-0.95</float>
                                                    <float id="width">1.00</float>
                                                    <float id="height">0.35</float>
                                                    <string id="check line of sight" values="yesno">no</string>
                                                    <string id="ignore invisibility" values="yesno">no</string>
                                                </condition>
                                            </or>
                                        </orblock>
                                    </else>
                                    <or>
                                        <condition id="isInArea">
                                            <string id="condition" values="yesno">yes</string>
                                            <string id="collision groups" values="collison groups" multiselect="true">INVINCIBLE_OBSTACLES;;DESTROYABLE_OBSTACLES;;</string>
                                            <string id="check green obstacles only" values="yesno">yes</string>
                                            <string id="teams" values="teams" multiselect="true">OWN_TEAM;;ENEMY_TEAM;;NEUTRAL_TEAM;;</string>
                                            <string id="areaName"></string>
                                            <float id="xOffset">1.50</float>
                                            <float id="yOffset">1.20</float>
                                            <float id="width">0.40</float>
                                            <float id="height">1.00</float>
                                            <string id="check line of sight" values="yesno">no</string>
                                            <string id="ignore invisibility" values="yesno">no</string>
                                        </condition>
                                        <condition id="isInArea">
                                            <string id="condition" values="yesno">yes</string>
                                            <string id="collision groups" values="collison groups" multiselect="true">INVINCIBLE_OBSTACLES;;DESTROYABLE_OBSTACLES;;</string>
                                            <string id="check green obstacles only" values="yesno">yes</string>
                                            <string id="teams" values="teams" multiselect="true">OWN_TEAM;;ENEMY_TEAM;;NEUTRAL_TEAM;;</string>
                                            <string id="areaName"></string>
                                            <float id="xOffset">-1.50</float>
                                            <float id="yOffset">1.20</float>
