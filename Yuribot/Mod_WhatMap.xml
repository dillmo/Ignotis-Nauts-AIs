                                            <string id="ignore invisibility" values="yesno">no</string>
                                        </condition>
                                    </or>
                                </orblock>
                            </normal>
                        </condition>
                    </normal>
                </condition>
            </normal>
        </root>
    </behaviour>
</enemy>
