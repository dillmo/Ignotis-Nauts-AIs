<?xml version="1.0" ?>
<enemy>
    <behaviour>
        <root x="110" y="40">
            <normal>
                <condition id="once">
                    <string id="Comment">Declares variables</string>
                    <normal>
                        <action id="setBool">
                            <string id="id">SelfAware_EnemyInFace</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                        <action id="setBool">
                            <string id="id">SelfAware_Hurt</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                        <action id="setBool">
                            <string id="id">SelfAware_Unhealthy</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                        <action id="setBool">
                            <string id="id">SelfAware_Critical</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                        <action id="setBool">
                            <string id="id">SelfAware_NearEnemyWeak</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                        <action id="setBool">
                            <string id="id">SelfAware_GangBangers</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                        <action id="setBool">
                            <string id="id">SelfAware_CanHitTarget</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                        <action id="setBool">
                            <string id="id">SelfAware_NearHurtAlly</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                    </normal>
                </condition>
                <andblock>
                    <string id="Comment">Are all towers down?</string>
                    <normal>
                        <action id="setBool">
                            <string id="id">enemyTowersDown</string>
                            <string id="value" values="flagtoggle">yes</string>
                        </action>
                    </normal>
                    <else>
                        <action id="setBool">
                            <string id="id">enemyTowersDown</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                    </else>
                    <or>
                        <condition id="getBoolEquals">
                            <string id="id">enemyTower1Lane1</string>
                            <string id="value" values="yesno">no</string>
                        </condition>
                        <condition id="getBoolEquals">
                            <string id="id">enemyTower2Lane1</string>
                            <string id="value" values="yesno">no</string>
                        </condition>
                        <condition id="getBoolEquals">
                            <string id="id">enemyTower1Lane2</string>
                            <string id="value" values="yesno">no</string>
                        </condition>
                        <condition id="getBoolEquals">
                            <string id="id">enemyTower2Lane2</string>
                            <string id="value" values="yesno">no</string>
                        </condition>
                    </or>
                </andblock>
                <condition id="isFromTeam">
                    <string id="team" values="teamswithnumbers">ZERO_TEAM</string>
                    <string id="check on" values="targetself">self</string>
                    <normal>
                        <condition id="isInArea">
                            <string id="condition" values="yesno">yes</string>
                            <string id="collision groups" values="collison groups" multiselect="true">NAMED_AREAS;;</string>
                            <string id="check green obstacles only" values="yesno">no</string>
                            <string id="teams" values="teams" multiselect="true"></string>
                            <string id="areaName">ATTACKBASE_RED</string>
                            <float id="xOffset">0.00</float>
                            <float id="yOffset">0.00</float>
                            <float id="width">0.00</float>
                            <float id="height">0.00</float>
                            <string id="check line of sight" values="yesno">no</string>
                            <string id="ignore invisibility" values="yesno">no</string>
                            <string id="Comment">has the red base alarm been tripped?</string>
                            <normal>
                                <action id="setBool">
                                    <string id="id">BaseTripped</string>
                                    <string id="value" values="flagtoggle">yes</string>
                                </action>
                            </normal>
                        </condition>
                    </normal>
                    <else>
                        <condition id="isInArea">
                            <string id="condition" values="yesno">yes</string>
                            <string id="collision groups" values="collison groups" multiselect="true">NAMED_AREAS;;</string>
                            <string id="check green obstacles only" values="yesno">no</string>
                            <string id="teams" values="teams" multiselect="true"></string>
                            <string id="areaName">ATTACKBASE_BLUE</string>
                            <float id="xOffset">0.00</float>
                            <float id="yOffset">0.00</float>
                            <float id="width">0.00</float>
                            <float id="height">0.00</float>
                            <string id="check line of sight" values="yesno">no</string>
                            <string id="ignore invisibility" values="yesno">no</string>
                            <string id="Comment">has the blue base alarm been tripped?</string>
                            <normal>
                                <action id="setBool">
                                    <string id="id">BaseTripped</string>
                                    <string id="value" values="flagtoggle">yes</string>
                                </action>
                            </normal>
                        </condition>
                    </else>
                </condition>
                <condition id="getBoolEquals">
                    <string id="id">2tick</string>
                    <string id="value" values="yesno">yes</string>
                    <string id="Comment">Information to update every 2 ticks</string>
                    <normal>
                        <orblock>
                            <normal>
                                <action id="setBool">
                                    <string id="id">SelfAware_EnemyInFace</string>
                                    <string id="value" values="flagtoggle">yes</string>
                                </action>
                            </normal>
                            <else>
                                <action id="setBool">
                                    <string id="id">SelfAware_EnemyInFace</string>
                                    <string id="value" values="flagtoggle">no</string>
                                </action>
                            </else>
                            <or>
                                <condition id="isInArea">
                                    <string id="condition" values="yesno">yes</string>
                                    <string id="collision groups" values="collison groups" multiselect="true">PLAYERS;;</string>
                                    <string id="check green obstacles only" values="yesno">no</string>
                                    <string id="teams" values="teams" multiselect="true">ENEMY_TEAM;;</string>
                                    <string id="areaName"></string>
                                    <float id="xOffset">0.00</float>
                                    <float id="yOffset">0.00</float>
                                    <float id="width">0.40</float>
                                    <float id="height">0.60</float>
                                    <string id="check line of sight" values="yesno">yes</string>
                                    <string id="ignore invisibility" values="yesno">no</string>
                                    <string id="Comment">Are enemies in my face? (Or above me)</string>
                                </condition>
                                <condition id="isInArea">
                                    <string id="condition" values="yesno">yes</string>
                                    <string id="collision groups" values="collison groups" multiselect="true">CREEPS;;</string>
                                    <string id="check green obstacles only" values="yesno">no</string>
                                    <string id="teams" values="teams" multiselect="true">ENEMY_TEAM;;</string>
                                    <string id="areaName"></string>
                                    <float id="xOffset">0.00</float>
                                    <float id="yOffset">0.00</float>
                                    <float id="width">0.20</float>
                                    <float id="height">0.20</float>
                                    <string id="check line of sight" values="yesno">yes</string>
                                    <string id="ignore invisibility" values="yesno">no</string>
                                    <string id="Comment">Are droids in my face? (Or above me)</string>
                                </condition>
                            </or>
                        </orblock>
                        <condition id="checkCharacterValue">
                            <string id="property" values="charactervaluesCheckable">health minus dot percentage</string>
                            <float id="value">90.00</float>
                            <string id="compare method" values="valuecompare">less or equal</string>
                            <string id="check on" values="targetself">self</string>
                            <string id="Comment">Am I hurt?</string>
                            <normal>
                                <action id="setBool">
                                    <string id="id">SelfAware_Hurt</string>
                                    <string id="value" values="flagtoggle">yes</string>
                                </action>
                            </normal>
                            <else>
                                <action id="setBool">
                                    <string id="id">SelfAware_Hurt</string>
                                    <string id="value" values="flagtoggle">no</string>
                                </action>
                            </else>
                        </condition>
                        <condition id="checkCharacterValue">
                            <string id="property" values="charactervaluesCheckable">health minus dot percentage</string>
                            <float id="value">50.00</float>
                            <string id="compare method" values="valuecompare">less or equal</string>
                            <string id="check on" values="targetself">self</string>
                            <string id="Comment">Am I unhealthy?</string>
                            <normal>
                                <action id="setBool">
                                    <string id="id">SelfAware_Unhealthy</string>
                                    <string id="value" values="flagtoggle">yes</string>
                                </action>
                            </normal>
                            <else>
                                <action id="setBool">
                                    <string id="id">SelfAware_Unhealthy</string>
                                    <string id="value" values="flagtoggle">no</string>
                                </action>
                            </else>
                        </condition>
                        <condition id="checkCharacterValue">
                            <string id="property" values="charactervaluesCheckable">health minus dot percentage</string>
                            <float id="value">35.00</float>
                            <string id="compare method" values="valuecompare">less or equal</string>
                            <string id="check on" values="targetself">self</string>
                            <string id="Comment">Am in critical condition?</string>
                            <normal>
                                <action id="setBool">
                                    <string id="id">SelfAware_Critical</string>
                                    <string id="value" values="flagtoggle">yes</string>
                                </action>
                            </normal>
                            <else>
                                <action id="setBool">
                                    <string id="id">SelfAware_Critical</string>
                                    <string id="value" values="flagtoggle">no</string>
                                </action>
                            </else>
                        </condition>
                        <andblock>
                            <string id="Comment">Can I hit my target?</string>
                            <normal>
                                <condition id="isUpgradeEnabled">
                                    <string id="condition" values="yesno">yes</string>
                                    <string id="upgrade name">JetterSummonerShotRange</string>
                                    <string id="Comment">Do I have long laser?</string>
                                    <normal>
                                        <action id="setBool">
                                            <string id="id">SelfAware_CanHitTarget</string>
                                            <string id="value" values="flagtoggle">yes</string>
                                        </action>
                                    </normal>
                                    <else>
                                        <andblock>
                                            <string id="Comment">Is it close enough to reach without long laser?</string>
                                            <normal>
                                                <action id="setBool">
                                                    <string id="id">SelfAware_CanHitTarget</string>
                                                    <string id="value" values="flagtoggle">yes</string>
                                                </action>
                                            </normal>
                                            <else>
                                                <action id="setBool">
                                                    <string id="id">SelfAware_CanHitTarget</string>
                                                    <string id="value" values="flagtoggle">no</string>
                                                </action>
                                            </else>
                                            <or>
                                                <condition id="isTargetInArea">
                                                    <string id="condition" values="yesno">yes</string>
                                                    <float id="xOffset">0.00</float>
                                                    <float id="yOffset">0.30</float>
                                                    <float id="width">0.95</float>
                                                    <float id="height">0.95</float>
                                                    <string id="check line of sight" values="yesno">no</string>
                                                    <string id="ignore invisibility" values="yesno">no</string>
                                                    <string id="Minimized">yes</string>
                                                </condition>
                                                <condition id="isTargetInArea">
                                                    <string id="condition" values="yesno">yes</string>
                                                    <float id="xOffset">0.00</float>
                                                    <float id="yOffset">0.00</float>
                                                    <float id="width">1.15</float>
                                                    <float id="height">1.15</float>
                                                    <string id="check line of sight" values="yesno">yes</string>
                                                    <string id="ignore invisibility" values="yesno">no</string>
                                                </condition>
                                            </or>
                                        </andblock>
                                    </else>
                                </condition>
                            </normal>
                            <else>
                                <action id="setBool">
                                    <string id="id">SelfAware_CanHitTarget</string>
                                    <string id="value" values="flagtoggle">no</string>
                                </action>
                            </else>
                            <or>
                                <condition id="isTargetInArea">
                                    <string id="condition" values="yesno">yes</string>
                                    <float id="xOffset">0.00</float>
                                    <float id="yOffset">0.00</float>
                                    <float id="width">1.30</float>
                                    <float id="height">1.30</float>
                                    <string id="check line of sight" values="yesno">yes</string>
                                    <string id="ignore invisibility" values="yesno">no</string>
                                </condition>
                                <condition id="isTargetInArea">
                                    <string id="condition" values="yesno">yes</string>
                                    <float id="xOffset">0.00</float>
                                    <float id="yOffset">0.30</float>
                                    <float id="width">1.00</float>
                                    <float id="height">1.00</float>
                                    <string id="check line of sight" values="yesno">no</string>
                                    <string id="ignore invisibility" values="yesno">no</string>
                                </condition>
                            </or>
                        </andblock>
                    </normal>
                </condition>
                <condition id="getBoolEquals">
                    <string id="id">3tick</string>
                    <string id="value" values="yesno">yes</string>
                    <string id="Comment">Information to update every 3 ticks</string>
                    <normal>
                        <condition id="isCharacterInArea">
                            <string id="groups" values="target receive groups" multiselect="true">PLAYERS;;</string>
                            <string id="teams" values="teams" multiselect="true">ENEMY_TEAM;;</string>
                            <string id="class"></string>
                            <string id="condition" values="charactervaluesCheckable">health minus dot percentage</string>
                            <string id="comparison" values="valuecompare">less or equal</string>
                            <float id="value">30.00</float>
                            <string id="character minimum">1</string>
                            <float id="xOffset">0.00</float>
                            <float id="yOffset">0.00</float>
                            <float id="width">1.00</float>
                            <float id="height">1.00</float>
                            <string id="check line of sight" values="yesno">yes</string>
                            <string id="Comment">Is a nearby enemy weak enough to kill?</string>
                            <normal>
                                <action id="setBool">
                                    <string id="id">SelfAware_NearEnemyWeak</string>
                                    <string id="value" values="flagtoggle">yes</string>
                                </action>
                            </normal>
                            <else>
                                <action id="setBool">
                                    <string id="id">SelfAware_NearEnemyWeak</string>
                                    <string id="value" values="flagtoggle">no</string>
                                </action>
                            </else>
                        </condition>
                        <condition id="isCharacterInArea">
                            <string id="groups" values="target receive groups" multiselect="true">PLAYERS;;</string>
                            <string id="teams" values="teams" multiselect="true">ENEMY_TEAM;;</string>
                            <string id="class"></string>
                            <string id="condition" values="charactervaluesCheckable">health minus dot percentage</string>
                            <string id="comparison" values="valuecompare">greater or equal</string>
                            <float id="value">15.00</float>
                            <string id="character minimum">2</string>
                            <float id="xOffset">0.00</float>
                            <float id="yOffset">0.00</float>
                            <float id="width">1.50</float>
                            <float id="height">1.50</float>
                            <string id="check line of sight" values="yesno">yes</string>
                            <string id="Comment">Am I about to get gang-banged?</string>
                            <string id="Minimized">yes</string>
                            <normal>
                                <action id="setBool">
                                    <string id="id">SelfAware_GangBangers</string>
                                    <string id="value" values="flagtoggle">yes</string>
                                </action>
                            </normal>
                            <else>
                                <action id="setBool">
                                    <string id="id">SelfAware_GangBangers</string>
                                    <string id="value" values="flagtoggle">no</string>
                                </action>
                            </else>
                        </condition>
                        <condition id="isCharacterInArea">
                            <string id="groups" values="target receive groups" multiselect="true">PLAYERS;;</string>
                            <string id="teams" values="teams" multiselect="true">OWN_TEAM;;</string>
                            <string id="class"></string>
                            <string id="condition" values="charactervaluesCheckable">health minus dot percentage</string>
                            <string id="comparison" values="valuecompare">less or equal</string>
                            <float id="value">80.00</float>
                            <string id="character minimum">1</string>
                            <float id="xOffset">0.00</float>
                            <float id="yOffset">0.00</float>
                            <float id="width">0.60</float>
                            <float id="height">0.60</float>
                            <string id="check line of sight" values="yesno">yes</string>
                            <string id="Comment">Is a nearby ally hurt?</string>
                            <normal>
                                <action id="setBool">
                                    <string id="id">SelfAware_NearHurtAlly</string>
                                    <string id="value" values="flagtoggle">yes</string>
                                </action>
                            </normal>
                            <else>
                                <action id="setBool">
                                    <string id="id">SelfAware_NearHurtAlly</string>
                                    <string id="value" values="flagtoggle">no</string>
                                </action>
                            </else>
                        </condition>
                    </normal>
                </condition>
                <condition id="getBoolEquals">
                    <string id="id">10tick</string>
                    <string id="value" values="yesno">yes</string>
                    <string id="Comment">information to update every 10 ticks</string>
                    <normal>
                        <condition id="getBoolEquals">
                            <string id="id">inField</string>
                            <string id="value" values="yesno">yes</string>
                            <string id="Comment">Updates inField</string>
                            <normal>
                                <orblock>
                                    <normal>
                                        <action id="setBool">
                                            <string id="id">inField</string>
                                            <string id="value" values="flagtoggle">no</string>
                                            <string id="Minimized">yes</string>
                                        </action>
                                    </normal>
                                    <or>
                                        <condition id="isInNamedArea">
                                            <string id="area name">STARTAREA</string>
                                            <string id="team" values="ownenemy">OWN_TEAM</string>
                                            <string id="who" values="targetself">self</string>
                                            <string id="Minimized">yes</string>
                                        </condition>
                                        <condition id="isInNamedArea">
                                            <string id="area name">HEALAREA</string>
                                            <string id="team" values="ownenemy">OWN_TEAM</string>
                                            <string id="who" values="targetself">self</string>
                                            <string id="Minimized">yes</string>
                                        </condition>
                                    </or>
                                </orblock>
                                <condition id="isCharacterInArea">
                                    <string id="groups" values="target receive groups" multiselect="true">PLAYERS;;CREEPS;;</string>
                                    <string id="teams" values="teams" multiselect="true">ENEMY_TEAM;;</string>
                                    <string id="class"></string>
                                    <string id="condition" values="charactervaluesCheckable">health</string>
                                    <string id="comparison" values="valuecompare">greater or equal</string>
                                    <float id="value">0.00</float>
                                    <string id="character minimum">1</string>
                                    <float id="xOffset">0.00</float>
                                    <float id="yOffset">0.00</float>
                                    <float id="width">4.00</float>
                                    <float id="height">4.00</float>
                                    <string id="check line of sight" values="yesno">no</string>
                                    <normal>
                                        <action id="setBool">
                                            <string id="id">inField</string>
                                            <string id="value" values="flagtoggle">no</string>
                                            <string id="Minimized">yes</string>
                                        </action>
                                    </normal>
                                </condition>
                            </normal>
                            <else>
                                <condition id="isCharacterInArea">
                                    <string id="groups" values="target receive groups" multiselect="true">PLAYERS;;CREEPS;;</string>
                                    <string id="teams" values="teams" multiselect="true">ENEMY_TEAM;;</string>
                                    <string id="class"></string>
                                    <string id="condition" values="charactervaluesCheckable">health</string>
                                    <string id="comparison" values="valuecompare">greater or equal</string>
                                    <float id="value">0.00</float>
                                    <string id="character minimum">1</string>
                                    <float id="xOffset">0.00</float>
                                    <float id="yOffset">0.00</float>
                                    <float id="width">2.00</float>
                                    <float id="height">2.00</float>
                                    <string id="check line of sight" values="yesno">no</string>
                                    <normal>
                                        <action id="setBool">
                                            <string id="id">inField</string>
                                            <string id="value" values="flagtoggle">yes</string>
                                            <string id="Minimized">yes</string>
                                        </action>
                                    </normal>
                                </condition>
                            </else>
                        </condition>
                    </normal>
                </condition>
            </normal>
        </root>
    </behaviour>
</enemy>
